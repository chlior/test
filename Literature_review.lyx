\begin{equation}
\frac{\partial}{{\partial t}}\left({\rho e}\right)+\frac{\partial}{{\partial{x_{i}}}}\left({\rho{u_{j}}e}\right)=-p\frac{{\partial{u_{i}}}}{{\partial{x_{i}}}}+{\tau_{ji}}\frac{{\partial{u_{i}}}}{{\partial{x_{j}}}}+\frac{{\partial{q_{i}}}}{{\partial{x_{i}}}}\label{eq:EnergyC}
\end{equation}

\end_inset


\end_layout

