issn = {0066-4189},
journal = {Annu. Rev. Fluid Mech},
keywords = {drag reduction,lotus effect,ultrahydrophobic},
number = {August 2009},
pages = {89--109},
title = {{Slip on Superhydrophobic Surfaces}},
